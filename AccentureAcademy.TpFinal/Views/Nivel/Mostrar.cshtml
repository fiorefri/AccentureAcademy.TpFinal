@using AccentureAcademy.TpFinal.Models
@model List<Niveles>

@{
    ViewBag.Title = "Niveles";
}

<h2>Mostrar</h2>

@if (this.Model.Count > 0)
{
    <ul>
        @foreach (Niveles nivel in this.Model)
        {
            <li>@nivel.Nivel</li>
        }
    </ul>
}
else
{
    <p>No se encuentran niveles para mostrar</p>
}
