@using AccentureAcademy.TpFinal.Models
@model List<Autores>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Mostrar Autores</title>
</head>
<body>
    <h1>Libros</h1>
    <div>
        <ul>
            @foreach (Autores autor in this.Model)
            {
                <li>@autor.Nombre  @autor.Apellido</li>
            }
        </ul>
    </div>
</body>
</html>
